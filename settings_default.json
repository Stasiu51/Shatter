{
  "features": {
    "terminalErrors": true
  },
  "appearance": {
    "focusDim": 0.5
  },
  "keybindings": {
    "commands": {
      "layer.prev": { "name": "Previous layer", "bindings": ["Q", "ArrowLeft"] },
      "layer.next": { "name": "Next layer", "bindings": ["E", "ArrowRight"] },
      "layer.prev.fast": { "name": "Previous layer (x5)", "bindings": ["Shift+Q", "Shift+ArrowLeft"] },
      "layer.next.fast": { "name": "Next layer (x5)", "bindings": ["Shift+E", "Shift+ArrowRight"] },
      "edit.undo": { "name": "Undo", "bindings": ["Mod+Z"] },
      "edit.redo": { "name": "Redo", "bindings": ["Shift+Mod+Z", "Mod+Y"] },
      "panel.zoom.in": { "name": "Panel zoom in", "bindings": ["Mod+="] },
      "panel.zoom.out": { "name": "Panel zoom out", "bindings": ["Mod+-"] },
      "panel.zoom.reset": { "name": "Panel zoom reset", "bindings": ["Mod+0"] }
      ,
      "layer.insert": { "name": "Insert layer", "bindings": ["Mod+Insert"] },
      "layer.delete": { "name": "Delete layer", "bindings": ["Mod+Delete"] }
    }
  }
}
